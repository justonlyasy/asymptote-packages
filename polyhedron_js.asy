polyhedron dodecahedron;
triple[] M_;
M_[0] = (1/3)*(M[0]+M[8]+M[2]);
M_[1] = (1/3)*(M[0]+M[4]+M[8]);
M_[2] = (1/3)*(M[0]+M[5]+M[4]);
M_[3] = (1/3)*(M[0]+M[9]+M[5]);
M_[4] = (1/3)*(M[0]+M[2]+M[9]);
