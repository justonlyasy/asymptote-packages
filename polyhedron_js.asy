M_[6] = (1/3)*(M[8]+M[10]+M[6]);
M_[7] = (1/3)*(M[8]+M[4]+M[10]);
M_[8] = (1/3)*(M[4]+M[1]+M[10]);
M_[9] = (1/3)*(M[4]+M[5]+M[1]);
M_[10] = (1/3)*(M[5]+M[11]+M[1]);
